{
  "name": "Sprint Graph",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "Visualize Jira Sprints as graphs",
  "background": {
    "scripts": [
      "backgroundScript.bundle.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "browser_action": {
    "default_title": "Sprint Graph"
  },
  "web_accessible_resources": [
    "*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
